int countingValleys(int n, char* s)
 {  
    
    int k=0,c=0,i;
    for(i=0;s[i] !='\0';i++)
    {
        if(s[i] == 'D')
            k--;
        else if(s[i] == 'U')
        {
            k++;
            if(k == 0 && s[i] == 'U')
                c++;
        }
           
    }
    return c;
 }
