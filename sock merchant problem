int sockMerchant(int n, int ar_count, int* ar) {
    int a=0,i,j;
    for(i=0;i<n;i++)
    {
          for(j=i+1;j<n;j++)
          {
             if(ar[i]==ar[j] && ar[j]!=0)
             {
                 a=a+1;
                 ar[j]=0;
                 j=n;
             }
             
          }
    }
    return a;
}
